  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexey-bass/angular-webpack-starter/issues"
  },
  "homepage": "https://github.com/alexey-bass/angular-webpack-starter",
  "devDependencies": {
    "babel-core": "6.3.x",
    "babel-loader": "6.2.x",
    "babel-preset-es2015": "6.3.x",
    "babel-preset-stage-1": "6.3.x",
    "css-loader": "0.23.x",
    "file-loader": "0.8.x",
    "html-minifier": "1.2.x",
    "html-webpack-plugin": "2.9.x",
    "json-loader": "0.5.x",
